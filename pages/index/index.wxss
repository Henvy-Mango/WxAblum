.back {
  height: 100%;
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-bottom: 20prx;
}

.tip_container{
  width: 640rpx;
  margin-top: 30rpx;
  box-shadow: 0rpx 0rpx 25rpx 3rpx rgb(214, 214, 214);
}
.tip {
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 20rpx;
}

.img1 {
  margin-top: 30rpx;
  width: 90%;
  border-radius: 20rpx;
}
.imgView{
  width: 100%;
}
.wrapper {
  display: flex;
  align-items: center;
  justify-content: center;
  height: 100%;
}

.textContent {
  width: 100%;
  height: 100rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  display: flex;
  flex-direction: column;
}

.text1 {
  width: 100%;
  height: 140rpx;
  box-sizing: border-box;
  padding: 35rpx;
}

.time {
  width: 100%;
  height: 60rpx;
  box-sizing: border-box;
  padding: 0rpx 35rpx 0 250rpx;
}

.btnArea {
  font-size: 30rpx;
  margin-top: 30rpx;
  width: 640rpx;
  border-radius: 20rpx;
  background-color: white;
  display: flex;
  flex-direction: column;
  align-items: center;
  box-shadow: 0rpx 0rpx 25rpx 3rpx rgb(214, 214, 214);
}

.navigateBtn {
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  border-radius: 20rpx;
}

.btn {
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: row;
  border-radius: 20rpx;
}

.imgArea1 {
  display: flex;
  align-items: center;
  justify-items: center;
}

.img2 {
  width: 40%;
  height: 40%;
}

.navigateName {
  height: 100%;
  display: flex;
  align-items: center;
  box-sizing: border-box;
  padding-left: 50rpx;
}

.imgArea2 {
  display: flex;
  align-items: center;
  justify-items: center;
}

.line {
  width: 90%;
  height: 1rpx;
  background-color: #d3d3d3;
}

.bottomText {
  font-size: 30rpx;
  color: rgb(88, 88, 88);
  width: 90%;
  height: 60rpx;
  position: absolute;
  bottom: 15rpx;
  display: flex;
  justify-content: center;
}